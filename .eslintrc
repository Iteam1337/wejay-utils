{
  "extends": ["@iteam/eslint-config-node", "prettier"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "overrides": [
    {
      "files": ["**/*.ts"],
      "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "react/prop-types": "off"
      }
    }
  ]
}
